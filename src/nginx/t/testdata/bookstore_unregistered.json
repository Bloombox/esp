{
 "name": "endpoints-test.cloudendpointsapis.com",
 "http": {
  "rules": [
   {
    "selector": "ListShelves",
    "get": "/shelves"
   },
   {
    "selector": "GetShelf",
    "get": "/shelves/{shelf}"
   },
   {
    "selector": "ListBooks",
    "get": "/shelves/{shelf}/books"
   },
   {
    "selector": "GetBook",
    "get": "/shelves/{shelf}/books/{book}"
   }
  ]
 },
 "usage": {
  "rules": [
   {
    "selector": "ListShelves",
    "allowUnregisteredCalls": true
   },
   {
    "selector": "GetShelf",
    "allowUnregisteredCalls": true
   },
   {
    "selector": "ListBooks",
    "allowUnregisteredCalls": false
   },
   {
    "selector": "GetBook",
    "allowUnregisteredCalls": false
   }
  ]
 },
 "control": {
  "environment": "http://127.0.0.1:8082"
 }
}
